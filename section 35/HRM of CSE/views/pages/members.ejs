<%- include('../partials/head') %>

    <title><%= category %></title>
</head>
<body>
    <%- include('../partials/navbar') %>
    
    <h1 class="d-flex justify-content-center my-4"><%= title %></h1>

    <ul class="d-grid gap-3 member-list">
        <% for (let member of members) { %>
            <li><a href="/<%= category %>/<%= member.id %>" class="member-name"><%= member.name %></a></li>
        <% } %>
    </ul>

    <div class="col-6 mx-auto text-center"> 
        <a href="/<%= category %>/new" class="btn btn-block btn-primary btn-lg mb-2">Add member</a>
    </div> 
</body>
</html>